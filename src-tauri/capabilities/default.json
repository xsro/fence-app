{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-open",
    "shell:allow-stdin-write",
    "fs:allow-app-read",
    {
      "identifier":"shell:allow-execute",
      "allow": [
        {
          "name": "python",
          "cmd":",",
          "args": true
        }
      ]
    }
  ]
}
